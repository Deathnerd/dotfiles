# vim: ft=tmux
# Session config for tmux 'tmux -f $XDG_CONFIG_HOME/tmux/vimb attach -t vimb'

source ~/.tmux.conf

new -d -c ~/code/c/vimb/src -s vimb -n vim
neww -d -c ~/code/c/vimb -n make
setw -t vimb:2 monitor-activity on
neww -d -c ~/code/c/vimb-page -n page
splitw -v -p 10 -t vimb:3
selectw -t vimb:2
