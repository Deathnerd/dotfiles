handler-add mailto=urxvt -e mutt %s
handler-add ftp=urxvt -e wget %s -P /tmp/vimb
nnoremap q1 :o https://github.com/fanglingsu/vimb<CR>
nnoremap q2 :o http://fanglingsu.github.io/vimb/<CR>
nnoremap gt :sh! xdotool key --window $VIMB_XID ctrl+shift+l<CR><Esc>
nnoremap gT :sh! xdotool key --window $VIMB_XID ctrl+shift+h<CR><Esc>
nnoremap 1gt :sh! xdotool key --window $VIMB_XID ctrl+1<CR><Esc>
nnoremap 2gt :sh! xdotool key --window $VIMB_XID ctrl+2<CR><Esc>
nnoremap 3gt :sh! xdotool key --window $VIMB_XID ctrl+3<CR><Esc>
nnoremap 4gt :sh! xdotool key --window $VIMB_XID ctrl+4<CR><Esc>
nnoremap 5gt :sh! xdotool key --window $VIMB_XID ctrl+5<CR><Esc>
nnoremap 6gt :sh! xdotool key --window $VIMB_XID ctrl+6<CR><Esc>
nnoremap 7gt :sh! xdotool key --window $VIMB_XID ctrl+7<CR><Esc>
nnoremap 8gt :sh! xdotool key --window $VIMB_XID ctrl+8<CR><Esc>
nnoremap 9gt :sh! xdotool key --window $VIMB_XID ctrl+9<CR><Esc>
nnoremap L ]]
nnoremap <F11> :set fullscreen!<CR>
nnoremap <F12> gF
nnoremap H [[
nnoremap S :set scripts!\|no! r<CR>
nnoremap ss :set stylesheet!<CR><Esc>
inoremap <C-E> <C-T>
set completion-font=DejaVu Sans Mono 10
set cookie-accept=origin
set cookie-expire-time=14400
set download-path=/tmp/vimb
set editor-command=urxvt -title scratchpad -geometry 80x20+40+60 -e vim %s
set fontsize=12
set header=DNT=1,Referer
set history-max-items=30000
set home-page=about:blank
set input-font-error=DejaVu Sans Mono Bold 10
set input-font-normal=DejaVu Sans Mono 10
set minimumfontsize=8
set monofontsize=12
set nextpattern=/\bweiter\b/i,/\bnext\b/i,/^(>\|>>\|»)$/,/^(>\|>>\|»)/,/(>\|>>\|»)$/,/\bmore\|mehr\b/i
set plugins=off
set previouspattern=/\bzurück\b/i,/\bprev\|previous\b/i,/^(<\|<<\|«)$/,/^(<\|<<\|«)/,/(<\|<<\|«)$/
set scripts=off
set status-font=DejaVu Sans Mono Bold 10
set status-ssl-font=DejaVu Sans Mono Bold 10
set status-sslinvalid-font=DejaVu Sans Mono Bold 10
set strict-focus=on
set stylesheet=off
set webinspector=on
shortcut-add ec=http://ecosia.org/search.php?q=$0
shortcut-add gg=https://www.google.de/search?q=$0&ie=UTF-8
shortcut-add le=http://dict.leo.org/ende?search=$0
shortcut-add rfc=http://tools.ietf.org/html/rfc$0
shortcut-add wb=http://www.woerterbuch.info/?s=dict&query=$0
shortcut-add wp=http://de.wikipedia.org/w/index.php?search=$0
shortcut-add cc=https://searchcode.com/?q=$0
augroup script
autocmd LoadCommited * set scripts=off
autocmd LoadCommited file://*,https://github.com/* set scripts=on
augroup end
